@{
}
@model List<CRM.ViewModels.OrderVM>
  <table id="downloadtable" class="table table-borderless table-centered align-middle table-nowrap mb-0">
                                <thead class="text-muted table-light">
                                    <tr>
                                        <th scope="col">@sharedResource["Date"]</th>
                                        <th scope="col">@sharedResource["Customer"]</th>
                                        <th scope="col">@sharedResource["Address"]</th>
                                        <th scope="col">@sharedResource["NetTotal"]</th>
                                       @* <th scope="col">@sharedResource["Delivered"]</th>
                                        <th scope="col">@sharedResource["Cancelled"]</th>*@
                                        <th scope="col">@sharedResource["Country"]</th>
                                        <th scope="col">@sharedResource["Status"]</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{

                                        foreach (var item in Model)

                                        {
                                            <tr>
                                                <td>
                                                    @item.OrderDate.ToShortDateString()
                                                </td>
                                                <td>
                                                    @item.CusName
                                                </td>
                                                <td>@item.CusAddress</td>
                                                <td>@item.NetOrder</td>
                                                
                                                <td>@item.Country</td>
                                                <td>@item.Status</td>
                                            </tr>

                                            <!-- end tr -->
                                        }
                                    }


                                </tbody><!-- end tbody -->
</table><!-- end table -->
